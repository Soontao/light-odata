{
  "name": "c4codata",
  "version": "1.4.1",
  "unpkg": "./lib/c4codata-umd.js",
  "bin": {
    "odata-js-generator": "lib/cli/index.js"
  },
  "devDependencies": {
    "@types/http-string-parser": "^0.0.29",
    "@types/jest": "^20.0.7",
    "@types/lodash": "^4.14.102",
    "@types/node": "^8.0.25",
    "@types/url-parse": "^1.1.0",
    "@types/uuid": "^3.4.3",
    "jest": "^24",
    "jest-cli": "^24",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.1",
    "standard-version": "^4.2.0",
    "ts-jest": "^24",
    "ts-loader": "4",
    "tslint": "^5.6.0",
    "tslint-loader": "^3.5.3",
    "typescript": "3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.23.0",
    "webpack-cli": "^3.1.2"
  },
  "typings": "lib/index.d.ts",
  "scripts": {
    "build:umd": "webpack",
    "clean": "rimraf lib",
    "build": "npm run clean && tsc && npm run build:umd",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage --no-cache",
    "coverage:watch": "npm run coverage -- --watch",
    "release": "standard-version --no-verify && git push --follow-tags origin master",
    "prepare": "npm run build && npm test"
  },
  "dependencies": {
    "@ungap/url-search-params": "^0.1.2",
    "buffer": "^5.0.8",
    "cli": "^1.0.1",
    "http-string-parser": "^0.0.6",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
    "url-parse": "^1.4.1",
    "uuid": "^3.3.2",
    "xml2js": "^0.4.19"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/tests/",
      "/node_modules/",
      "/src/cli",
      "/src/generator"
    ],
    "testEnvironment": "node",
    "testRegex": "tests/.*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "description": "c4c odata javascript library",
  "homepage": "https://github.com/Soontao/c4codata",
  "author": {
    "name": "Theo Sun",
    "email": "theo.sun@sap.com",
    "url": "https://github.com/Soontao"
  },
  "main": "lib/index.js",
  "keywords": [
    "C4C",
    "SAP",
    "OData"
  ],
  "repository": "Soontao/c4codata",
  "license": "MIT"
}
